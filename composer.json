{
  "name": "hendrahuang/cassandra-bundle",
  "type" : "symfony-bundle",
  "description": "Symfony2 bundle on top of datastax/php-driver",
  "keywords": ["cassandra", "bundle"],
  "license" : "MIT",
  "authors": [
    {
      "name": "Hendra-Huang",
      "email" : "hendrahuang92@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-cassandra": "*"
  },
  "require-dev": {
    "atoum/atoum": "~2.0",
    "symfony/dependency-injection": "~2.3",
    "symfony/event-dispatcher": "~2.3",
    "symfony/config" : "~2.3",
    "symfony/yaml" : "~2.3",
    "datastax/php-driver": "dev-master"
  },
  "autoload": {
    "psr-4": {"CassandraBundle\\": "src/"}
  },
  "config": {
    "bin-dir": "bin/"
  },
  "minimum-stability": "dev"
}
